
<!-- include header -->
<%-include('_register-header')%>
<!-- include header -->
<div class="sign-up-container">
    <div class="sign-up-header">
        <h3 class="text">TOOTHFULLY YOURS</h3>
        <P class="text">Dental Clinic</P>
    </div>

    <div class="sign-up-body">
        <div class="row1">
            <h3>Create An Account</h3>
            <div class="social-media-icons">
                <img class="facebook" src="/img/facebook-icon.png">
                <img class="twitter" src="/img/twitter-icon.png">
            </div>
        </div>
        <form action="" method="POST">

            <div class="form-group">
                <input type="text" name="p_username" placeholder="Username">
                <input type="email" name="p_email" placeholder="email@example">
                <input type="password" name="p_password" placeholder="Password">
                <input type="password" name="p_cpassword" placeholder="Confirm Password">
            </div>

            <div class="form-group">
                <div class="terms-services">
                    <input class="check" name="terms "type="checkbox" >
                    <p>I agree in all statements in <a href="">Terms & Services.</a></p>
                </div>
            </div>

            <div class="form-group">
                <a href="">
                <button type="button" class="Confirm">Confirm</button>
                </a>
            </div> 

        </form>
       
        <div class="row5">
           <a href="/">Already a registered user?</a>
        </div>
    </div>

</div>

</div>


<!-- include footer -->
<%-include('include/_footer')%>
<!-- include footer --> 